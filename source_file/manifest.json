{
    "manifest_version":2,
    "name":"Saffron Extension Downloader",
    "version":"0.001",
    "content_scripts":[
        {
            "run_at" : "document_idle",
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "content.js"
            ]
        }
    ],
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "images/a2.png",
        "default_popup": "popup.html"
    }
}
